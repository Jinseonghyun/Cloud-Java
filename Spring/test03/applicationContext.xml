<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!--  이창진, 경기도 하남시, 010-1111-1111 -->
	<!--  김창진, 서울시 강남구, 010-2222-2222 -->
	<!-- 해당 값들이 저장된 빈 2개가 생성될 수 있도록 bean태그 추가 -->

	<bean id="lee" class="com.test03.Address">
		<constructor-arg name="name" value="이창진" /> <!-- constructor-arg 는 생성자 이용하는거   -->
		<constructor-arg name="addr" value="경기도 하남시" />
		<constructor-arg name="phone" value="010-1111-1111" />
	</bean>
</beans> 

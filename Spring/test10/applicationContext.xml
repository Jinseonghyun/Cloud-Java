<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="lee" class="com.test10.Student">
		<property name="name" value="이창진"></property>
		<property name="age" value="20"></property>
	</bean>
	<bean id="kim" class="com.test10.Student">
		<property name="name" value="김창진"></property>
		<property name="age" value="21"></property>
	</bean>
	
	<bean id="mySchool" class="com.test10.School">
		<property name="grade" value="1" />
	</bean>
</beans>
